{
  "name": "no-drama-ollama",
  "description": "ðŸ¦™ Minimalist web client for Ollama.",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/no-drama-ollama/graphs/contributors"
    }
  ],
  "version": "0.1.0",
  "homepage": "https://github.com/rooseveltframework/no-drama-ollama",
  "license": "CC-BY-4.0",
  "main": "no-drama-ollama.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "check-if-css-is-disabled": "2.0.2",
    "ollama": "0.6.2",
    "roosevelt": "0.31.4",
    "sass": "1.94.0",
    "semantic-forms": "5.3.1",
    "showdown": "2.1.0",
    "teddy": "1.1.4",
    "webpack": "5.102.1"
  },
  "devDependencies": {
    "nodemon": "3.1.11",
    "standard": "17.1.2",
    "stylelint": "16.25.0",
    "stylelint-config-standard-scss": "16.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/"
    ],
    "ext": "scss html js json"
  },
  "standard": {
    "ignore": [
      "public/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/no-drama-ollama.git"
  },
  "scripts": {
    "generate-secrets": "node ./node_modules/roosevelt/lib/scripts/secretsGenerator.js",
    "generate-certs": "node ./node_modules/roosevelt/lib/scripts/certsGenerator.js",
    "generate-session-secret": "node ./node_modules/roosevelt/lib/scripts/sessionSecretGenerator.js",
    "generate-csrf-secret": "node ./node_modules/roosevelt/lib/scripts/csrfSecretGenerator.js",
    "d": "nodemon no-drama-ollama.js --development-mode",
    "dev": "nodemon no-drama-ollama.js --development-mode",
    "development": "nodemon no-drama-ollama.js --development-mode",
    "p": "nodemon no-drama-ollama.js --production-mode",
    "prod": "nodemon no-drama-ollama.js --production-mode",
    "production": "nodemon no-drama-ollama.js --production-mode",
    "x": "nodemon no-drama-ollama.js --production-proxy-mode",
    "prodproxy": "nodemon no-drama-ollama.js --production-proxy-mode",
    "production-proxy": "nodemon no-drama-ollama.js --production-proxy-mode",
    "lint": "npm run standard && npm run stylelint",
    "standard": "./node_modules/.bin/standard",
    "stylelint": "./node_modules/.bin/stylelint \"statics/css/**/*.scss\"",
    "start": "nodemon no-drama-ollama.js --production-mode",
    "test": "npm run lint"
  },
  "keywords": [
    "ollama",
    "ollama web client",
    "ollama roosevelt",
    "ollama express"
  ],
  "funding": "https://www.paypal.com/donate/?hosted_button_id=2L2X8GRXZCGJ6"
}
